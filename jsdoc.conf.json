{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["frontend"],
      "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": ["frontend/node_modules"]

    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/frontend/jsdoc/",
        "recurse": true,
        "verbose": true
        }
  }